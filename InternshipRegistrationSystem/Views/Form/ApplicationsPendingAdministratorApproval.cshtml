@model IList<RegistrationForm>

<table class="table table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>Başlık</th>
            <th>Açıklama</th>
            <th>Durum</th>
            <th>#</th>
        </tr>
    </thead>
    <tbody>
        @if(Model is not null)
        {
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.RegistrationFormId</td>
                    <td>@item.Title</td>
                    <td>@item.Description</td>
                    <td>@item.Status!.Name</td>
                    <td>
                        <div class="btn-group">
                            <a asp-area="" asp-controller="Form" asp-action="AdministratorApprove" asp-route-id="@item.RegistrationFormId" class="btn btn-primary">Onayla</a>
                            <a asp-area="" asp-controller="Form" asp-action="AdministratorReject" asp-route-id="@item.RegistrationFormId" class="btn btn-outline-primary">Reddet</a>
                        </div>
                    </td>
                </tr>
            }
        }
    </tbody>
</table>